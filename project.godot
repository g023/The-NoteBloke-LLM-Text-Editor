; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="The NoteBloke LLM Text Editor"
config/description="An advanced Ai assisted text editing experience. The NoteBloke LLM Text Editor is packed with a ton of LLM Ai agent features to superpower your text editing."
config/version="0.01a"
run/main_scene="res://wind_test.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
config/icon="res://logo.png"

[display]

window/size/viewport_width=1600
window/size/viewport_height=980

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
